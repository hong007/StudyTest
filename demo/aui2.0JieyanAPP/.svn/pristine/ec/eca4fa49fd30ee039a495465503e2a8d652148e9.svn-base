/*选择地址*/
.suggest {
	position: fixed;
	top: 64px;
	transform: translateX(100%) translateZ(0);
	-webkit-transform: translateX(100%) translateZ(0);
	width: 100%;	
	z-index: 1000;
	background-color: #fafafa;
	transition: all .2s ease-out;
	-webkit-transition: all .2s ease-out
}
.suggest.visible {
	transform: translateX(0) translateZ(0);
	-webkit-transform: translateX(0) translateZ(0)
}
.suggest.hidden {
	transform: translateX(100%) translateZ(0);
	-webkit-transform: translateX(100%) translateZ(0)
}
.suggest .option {
	z-index: 20;
	height: 40px;
	line-height: 40px;
	position: relative;
	background-color: #fafafa;
	font-size: 14px;
}
.suggest .option:before, .suggest .option:after {
	display: block;
	position: absolute;
	border-top: 1px solid #e5e5e5;
	left: 0;
	width: 100%;
	content: ' '
}
.suggest .option:before {
	display: none;
	top: 0
}
.suggest .option:after {
	display: block;
	bottom: 0
}
.suggest .option .cancel {
	float: right;
	margin-right: 16px;
	color: #fa8919
}
.suggest .option input {
	width: 100%;
	line-height: 18px;
	outline: 0;
	opacity: .98;
	background-color: #fafafa;
	color: #666
}
.suggest .option input::-webkit-input-placeholder {
	color: #ccc
}
.suggest .option .city-select {
	display: inline-block;
	position: absolute;
	padding: 0 4px 0 16px;
	vertical-align: top;
	color: #000;
}
.suggest .option .city-select .cityname {
	vertical-align: top;
	display:inline-block;
	overflow: hidden;
	text-overflow:ellipsis;
	width:60px;
	white-space: nowrap;
}
.suggest .option .city-select .i-arrow-down {
	display: inline-block;
	vertical-align: top;
	width: 16px;
	height: 16px;
	margin-left: 4px;
	color: #ccc
}
.suggest .option .city-select .city-input {
	padding: 16px 0
}
.suggest .option .address-select {
	display: inline-block;
	position: absolute;
	right: 50px;
	line-height: 35px;
	padding-left: 10px;
	overflow: hidden;
	transition: all .1s ease-out;
	-webkit-transition: all .1s ease-out;
	vertical-align: top;
	border-left: 1px solid #e5e5e5
}
.suggest .option .address-select .address-input[type=text] {
	border-radius: 0;
	padding: 0;
	margin:0;
	line-height: 40px;
	border: none;
}
.suggest .pos-wrapper {
	position: absolute;
	width: 100%;
	top: 50px;
	bottom: 0
}
.suggest .pos-wrapper li {
	padding-left: 16px;
	height: 60px
}
.suggest .pos-wrapper li:active {
	background-color: #f0f0f0
}
.suggest .pos-wrapper li .name {
	padding-top: 22px;
	display: block;
	margin-bottom: 6px;
	font-size: 16px;
	color: #333
}
.suggest .pos-wrapper li .address {
	display: block;
	padding-bottom: 13px;
	font-size: 12px;
	color: #adadad;
	position: relative
}
.suggest .pos-wrapper li .address:before, .suggest .pos-wrapper li .address:after {
	display: block;
	position: absolute;
	border-top: 1px solid #e5e5e5;
	left: 0;
	width: 100%;
	content: ' '
}
.suggest .pos-wrapper li .address:before {
	display: none;
	top: 0
}
.suggest .pos-wrapper li .address:after {
	display: block;
	bottom: 0
}
.suggest .pos-wrapper li.has-address .name {
	padding-top: 13px
}
.suggest .pos-wrapper .poi-fail {
	text-align: center;
	padding: 20px;
	color: #adadad
}
.suggest .pos-wrapper .poi-fail .title {
	font-size: 16px;
	margin-bottom: 8px
}
.suggest .pos-wrapper .poi-fail .desc {
	font-size: 12px;
}
.suggest .pos-wrapper .tip-loading {
	padding-top: 8px;
	font-size: 14px
}
.suggest .pos-wrapper .no-click {
	color: #ccc
}
.suggest .pos-wrapper .no-click:after {
	display: none
}
.suggest .pos-wrapper .no-click:active {
	background-color: #fafafa
}
.suggest .pos-wrapper .no-result {
	padding-top: 50px;
	margin-left: -16px;
	text-align: center;
	font-size: 14px
}
.suggest .pos-wrapper .loading {
	display: inline-block;
	position: relative;
	top: 2px;
	width: 16px;
	height: 16px;
	margin-right: 10px;
	opacity: .2;	
	background-repeat: no-repeat;
	background-size: contain
}
